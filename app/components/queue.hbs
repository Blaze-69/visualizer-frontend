<section style="margin-top:12px;">
    <nav class="navbar navbar-light bg-light">
        <div class="container-fluid">
            <div class="d-flex">
                <input class="form-control me-2" placeholder="Value" aria-label="Insert" id="number">
                <button class="btn btn-success mx-2" id="insert">Enqueue</button>
                <button class="btn btn-warning mx-2" type="submit" id="delete">Dequeue</button>
                <button id ="stop" class="btn btn-danger mx-2">Stop</button>
            </div>
            <div class="d-flex justify-content-end">
                <label class="mx-2 mt-2">Speed: 1x</label>
                <input id="slider" type="range" class="form-range mt-2" min="0.1" max="5" step="0.1" style="width:270px">
                <label class="mx-2  mt-2">5x</label>
                <button id ="reset" class="btn btn-dark mx-2">Reset</button>
            </div>
        </div>
    </nav>

    <canvas></canvas>
    <div class="alert algo alert-danger" role="alert"></div>
</section>
